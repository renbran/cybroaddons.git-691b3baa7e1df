<?xml version="1.0" encoding="UTF-8" ?>
<templates id="template" xml:space="preserve">
    <t t-name="mail.DiscussSidebar.startMeetingButton"
       t-inherit="mail.DiscussSidebar.startMeetingButton"
       t-inherit-mode="extension">
    <xpath expr="//button" position="replace">
        <div t-on-click="store.startMeeting" data-hotkey="m"
             t-ref="meeting-btn"><i class="fa fa-video-camera"/>
            <span t-if="!store.discuss.isSidebarCompact"/></div>
    </xpath>
</t>
</templates>