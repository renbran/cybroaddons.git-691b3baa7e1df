<?xml version="1.0" encoding="UTF-8" ?>
<!--Defines a template for a document preview.-->
<template id="expiry_dashboard_template">
    <t t-name="DocumentPreviewTemplate" owl="1">
        <div class="container">
            <div class="document_preview_template">
                <t t-set="image" t-value="this.stamp"/>
                <img t-ref="document_preview_image" t-if='image' t-attf-src="data:image/png;base64,{{image}}"
                     alt="Logo" style="margin-left:75%;"/>
                <div t-ref='document_preview_template_content' class="document_preview_template_content">
                </div>
            </div>
        </div>
    </t>
</template>
