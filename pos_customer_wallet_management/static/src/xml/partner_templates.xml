<?xml version="1.0" encoding="UTF-8"?>
<!--Wallet recharge button in partner line-->
<templates id="template" xml:space="preserve">
    <t t-name="pos_customer_wallet_management.PartnerLine" t-inherit="point_of_sale.PartnerLine"
       t-inherit-mode="extension" owl="1">
        <xpath expr="//button[hasclass('unselect-tag')]" position="after">
                  <button class="wallet-recharge-button"
                          t-on-click.stop="onClickWallet">WALLET RECHARGE
                  </button>
        </xpath>
    </t>
</templates>
