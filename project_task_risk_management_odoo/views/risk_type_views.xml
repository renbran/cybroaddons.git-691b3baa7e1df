<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    Risk Type list View -->
    <record id="risk_type_view_list" model="ir.ui.view">
        <field name="name">risk.type.view.list</field>
        <field name="model">risk.type</field>
        <field name="arch" type="xml">
            <list string="Risk Type" editable="top">
                <field name="name"/>
                <field name="code"/>
            </list>
        </field>
    </record>
</odoo>