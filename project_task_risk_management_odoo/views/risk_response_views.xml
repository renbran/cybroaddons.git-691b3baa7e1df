<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--    Risk Response list View-->
    <record id="risk_response_view_list" model="ir.ui.view">
        <field name="name">risk.response.view.list</field>
        <field name="model">risk.response</field>
        <field name="arch" type="xml">
            <list string="Risk Response" editable="top">
                <field name="name"/>
                <field name="code"/>
            </list>
        </field>
    </record>

</odoo>