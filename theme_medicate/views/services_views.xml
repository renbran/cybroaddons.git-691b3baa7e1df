<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="theme_medicate.services_page" model="website.page">
        <field name="name">Services</field>
        <field name="type">qweb</field>
        <field name="url">/services</field>
        <field name="website_indexed" eval="False"/>
        <field name="is_published">True</field>
        <field name="key">theme_medicate.services_page</field>
        <field name="arch" type="xml">
            <t name="Services" t-name="theme_medicate.services_page">
                <t t-call="website.layout">
                    <div id="wrap" class="oe_structure oe_empty">
                        <main>
                        </main>
                    </div>
                </t>
            </t>
        </field>
    </record>
    <template id="load_services_snippet" inherit_id="theme_medicate.services_page_ir_ui_view">
        <xpath expr="//main" position="inside">
            <div class="oe_structure"/>
            <t t-call="theme_medicate.services_banner"/>
            <t t-call="theme_medicate.services_container"/>
        </xpath>
    </template>
</odoo>
